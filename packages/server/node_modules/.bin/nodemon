#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/nodemon@2.0.12/node_modules/nodemon/bin/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/nodemon@2.0.12/node_modules/nodemon/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/nodemon@2.0.12/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/node_modules:/home/marcus/Área de Trabalho/HiringCoders/node_modules:/home/marcus/Área de Trabalho/node_modules:/home/marcus/node_modules:/home/node_modules:/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/nodemon/bin/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/nodemon/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/nodemon@2.0.12/node_modules/nodemon/bin/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/nodemon@2.0.12/node_modules/nodemon/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/nodemon@2.0.12/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/node_modules:/home/marcus/Área de Trabalho/HiringCoders/node_modules:/home/marcus/Área de Trabalho/node_modules:/home/marcus/node_modules:/home/node_modules:/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/nodemon/bin/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/nodemon/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
else
  exec node  "$basedir/../nodemon/bin/nodemon.js" "$@"
fi
