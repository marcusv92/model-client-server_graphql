  export NODE_PATH="$NODE_PATH:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/mocha@9.0.3/node_modules/mocha/bin/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/mocha@9.0.3/node_modules/mocha/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/mocha@9.0.3/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/node_modules:/home/marcus/Área de Trabalho/HiringCoders/node_modules:/home/marcus/Área de Trabalho/node_modules:/home/marcus/node_modules:/home/node_modules:/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/riak-dc@0.3.2/node_modules/mocha/bin/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/riak-dc@0.3.2/node_modules/mocha/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/riak-dc@0.3.2/node_modules:/home/marcus/Área de Trabalho/HiringCoders/projetos/cliente-servidor-aula/packages/server/node_modules/.pnpm/riak-dc@0.3.2/node_modules/riak-dc/node_modules"
